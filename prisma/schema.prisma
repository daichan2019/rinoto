generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PresetAudio {
  id        String   @id @default(cuid())
  name      String
  path      String
  duration  Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("preset_audios")
}
